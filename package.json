{
  "name": "api.adamsanderson.co.uk",
  "version": "1.2.0",
  "description": "API to access and create content for adamsanderson.co.uk",
  "main": "app.js",
  "scripts": {
    "test": "ava \"test/**/*.test.js\"",
    "coverage": "npm test",
    "lint": "eslint app/**/* *.js controllers/**/* test/**/*",
    "start": "node api_server.js",
    "precommit": "npm run lint",
    "prepush": "npm run coverage",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "depcheck": "depcheck --ignores=\"eslint-config-google, eslint-config-node-style-guide, husky\""
  },
  "author": "Adam Sanderson",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.16.0",
    "depcheck": "^0.6.4",
    "eslint": "^3.2.1",
    "eslint-config-google": "^0.6.0",
    "eslint-config-node-style-guide": "^3.0.0",
    "flow-bin": "^0.37.4",
    "husky": "^0.11.6",
    "jsonschema": "^1.1.0",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.5",
    "swagger-tools": "^0.10.1",
    "swagger-validate": "^0.1.7"
  },
  "dependencies": {
    "bwip-js": "^1.0.4",
    "fleek-router": "^1.1.3",
    "hangouts-bot": "^0.2.1",
    "image-data-uri": "^1.0.0",
    "koa": "^1.2.1",
    "node-fetch": "^1.6.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "remark": "^6.0.0"
  }
}

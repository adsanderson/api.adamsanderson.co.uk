{
  "name": "api.adamsanderson.co.uk",
  "version": "1.2.0",
  "description": "API to access and create content for adamsanderson.co.uk",
  "main": "app.js",
  "scripts": {
    "test": "ava \"test/**/*.test.js\"",
    "coverage": "npm test",
    "lint": "lint-staged",
    "lint:standard": "standard \"app/**/*.js\" \"test/**/*.js\" \"controllers/**/*.js\"",
    "start": "node api_server.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "depcheck": "depcheck --ignores=\"eslint-config-google, eslint-config-node-style-guide, husky\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=7.6.0 <8.0.0"
  },
  "author": "Adam Sanderson",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.18.2",
    "depcheck": "^0.6.4",
    "flow-bin": "^0.40.0",
    "husky": "^0.13.1",
    "jsonschema": "^1.1.0",
    "lint-staged": "^3.3.1",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.5",
    "standard": "^9.0.0",
    "swagger-tools": "^0.10.1",
    "swagger-validate": "^0.1.7"
  },
  "dependencies": {
    "bwip-js": "^1.5.3",
    "fleek-context": "^1.0.4",
    "fleek-router": "^2.0.2",
    "image-data-uri": "^1.0.0",
    "koa": "^2.0.1",
    "node-fetch": "^1.6.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "remark": "^7.0.0"
  }
}
